@model List<TimeOnly>

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
</head>
<body>
    <h2>Шаг два</h2>
    <h2>Введенное время: @ViewData["date"]</h2>
    <h2>Введенный телефон: @ViewData["phone"]</h2>
    <h2>Введенная разновидность животного: @ViewData["kindOfAnimal"]</h2>

    <form method='post'>
        <label for="time">Выберите время:</label>
        <select input type="text" name="time">
            @foreach (var breed in Model)
            {
                <option value=@breed>@breed</option>
            }
        </select>

        <input type='submit' value='Отправить запрос' />
    </form>

</body>
</html>